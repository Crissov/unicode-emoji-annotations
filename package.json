{
  "name": "unicode-emoji-annotations",
  "version": "0.0.0",
  "private": true,
  "description": "unicode compliant internationalized emoji annotations",
  "author": "Andreas Tietz https://github.com/dematerializer",
  "main": "lib/index.js",
  "files": ["lib", "res"],
  "scripts": {
    "update-local-deps": "rm -rf node_modules/unicode-emoji-data; npm i",
    "start": "npm run build:annotations",
    "build:annotations": "NODE_ENV=production node build-emoji-annotations.js",
    "build:community": "node build-community-annotations.js",
    "test": "NODE_ENV=test nyc --reporter=lcov --reporter=text mocha --require ./test-env --compilers js:babel-core/register src/**/*.test.js",
    "posttest": "NODE_ENV=test npm run lint",
    "lint": "NODE_ENV=test eslint --ignore-path .gitignore .",
    "prepublish": "npm run test && npm run build:sources",
    "build:sources": "rm -rf lib; mkdir lib; babel src/index.js -o lib/index.js; babel src/combine-annotations.js -o lib/combine-annotations.js; babel src/utils.js -o lib/utils.js"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-plugin-istanbul": "^2.0.1",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "co": "^4.6.0",
    "es6-promisify": "^5.0.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb-base": "^9.0.0",
    "eslint-plugin-import": "^2.0.1",
    "i18n-iso-countries": "^1.6.0",
    "isomorphic-fetch": "^2.2.1",
    "left-pad": "^1.1.1",
    "log-update": "^1.0.2",
    "mocha": "^3.0.2",
    "nyc": "^8.3.0",
    "punycode": "^2.0.0",
    "unicode-emoji-data": "../unicode-emoji-data",
    "xml2js": "^0.4.17"
  }
}
